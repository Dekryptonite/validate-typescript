{
  "name": "validate-typescript",
  "version": "1.9.0",
  "description": "Extensible schema based validator that supports typescript typing.",
  "main": "./index.js",
  "types": "./index.d.ts",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf *.js *.js.map *.d.ts",
    "prepublishOnly": "npm run clean && npm run build",
