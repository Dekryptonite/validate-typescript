  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Dekryptonite/validate-typescript.git"
  },
  "keywords": [
    "validate",
    "validator",
    "validation",
    "typescript",
    "schema",
    "javascript"
  ],
  "author": "Dekryptonite",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Dekryptonite/validate-typescript/issues"
  },
  "homepage": "https://github.com/Dekryptonite/validate-typescript#readme",
  "dependencies": {
    "@types/chalk": "^2.2.0",
    "chalk": "^2.3.1"
  }
}
